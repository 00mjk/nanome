/*
 * -------------------------------------------------
 *  Nextflow config file for running benchmarking
 * -------------------------------------------------
 * Defines bundled input files and everything required
 * to run a fast and simple test.
 */

params {
	// Methylation calling params
    dsname = 'BenchmarkData'
    input = "test/benchmark.filelist.txt"

	// Running steps
    benchmarking=true
    eval=false
}