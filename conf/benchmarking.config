/*
 * -------------------------------------------------
 *  Nextflow config file for benchmarking
 * -------------------------------------------------
 * Defines bundled input files and everything required
 * to run a fast and simple test.
 */

params {
	// Methylation calling input params
	dsname = 'BenchmarkData'
	input = "inputs/benchmark.filelist.txt"
	processors = 8
	// Running steps configuration
	eval=false
}